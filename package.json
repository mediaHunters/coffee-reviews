{
  "name": "coffee-reviews",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "default": "./dist/index.cjs",
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "node": "./dist/index.cjs"
  },
  "scripts": {
    "dev:start": "wireit",
    "prod:start": "wireit",
    "build": "wireit",
    "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/index.ts",
    "format": "prettier **/*.ts --write",
    "lint": "wireit",
    "db:seed": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register -r dotenv/config node_modules/typeorm-seeding/dist/cli.js seed -n ormconfig.cjs -c default",
    "db:reload": "cross-env NODE_ENV=development ts-node src/infrastructure/database/cli/dbReload.ts reload",
    "db:migrate": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register node_modules\\typeorm\\cli.js migration:run",
    "db:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
    "db:packet": " npm run db:reload && npm run db:migrate && npm run db:seed",
    "clean": "wireit",
    "clean:deep": "wireit"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@loopback/rest": "^12.0.9",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.36",
    "@types/bcrypt": "^3.0.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@wufe/mapper": "^0.5.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "bcrypt": "^5.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "express": "~4.17.1",
    "express-fileupload": "^1.4.0",
    "helmet": "^3.21.2",
    "http-status-codes": "^1.4.0",
    "husky": "^3.1.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.3.2",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^7.3.0",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "node-fetch": "^3.0.0-beta.10",
    "nodemon": "^2.0.2",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "pngjs": "^7.0.0",
    "prettier": "^2.8.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "routing-controllers": "^0.10.2",
    "swagger-ui-express": "^4.1.3",
    "ts-node": "^10.9.1",
    "ttypescript": "^1.5.15",
    "typeorm-seeding": "^1.6.1",
    "typeorm-transactional-cls-hooked": "^0.1.20",
    "typescript": "^4.9.5",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.7.1",
    "wireit": "^0.9.5",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/express-serve-static-core": "^4.17.30",
    "@types/faker": "^4.1.9",
    "@types/helmet": "^0.0.45",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.7",
    "@types/node": "^13.13.52",
    "@types/pngjs": "^6.0.1",
    "@types/sinon": "^7.5.1",
    "@types/swagger-ui-express": "^4.1.1",
    "@types/yargs": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "cross-env": "^7.0.3",
    "tsconfig-paths": "^3.14.1",
    "tsup": "^6.6.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "wireit": {
    "dev:start": {
      "command": "cross-env NODE_ENV=development nodemon --experimental-modules --es-module-specifier-resolution=node dist/index.ts",
      "dependencies": [
        "dev:db:migrate"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod:start": {
      "command": "node dist/index.js",
      "dependencies": [
        "build",
        "prod:db:migrate"
      ],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "rimraf dist && swc ./src -d dist && copyfiles -f src/ui/common/config/*.json dist/ui/common/config/"
    },
    "dev:db:migrate": {
      "command": "npm run db:migrate",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod:db:migrate": {
      "command": "cross-env NODE_ENV=production npm run db:migrate",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint": {
      "command": "eslint --fix --color --max-warnings 0 --cache --cache-location .eslintcache .",
      "files": [
        "src/**/*.ts",
        ".eslintignore",
        ".eslintrc.js"
      ]
    },
    "clean": {
      "command": "rimraf dist"
    },
    "clean:deep": {
      "command": "rimraf node_modules .wireit",
      "dependencies": [
        "clean"
      ]
    }
  }
}
